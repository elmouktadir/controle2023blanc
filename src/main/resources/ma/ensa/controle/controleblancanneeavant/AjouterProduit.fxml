<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ma.ensa.controle.controleblancanneeavant.ProduitController">
   <children>
      <Label layoutX="241.0" layoutY="14.0" text="Ajouter Produit">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="refField" layoutX="226.0" layoutY="126.0" prefHeight="25.0" prefWidth="200.0" promptText="reference">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="prixField" layoutX="226.0" layoutY="162.0" prefHeight="25.0" prefWidth="200.0" promptText="prix">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </TextField>
      <TextArea fx:id="desField" layoutX="226.0" layoutY="200.0" prefHeight="132.0" prefWidth="200.0" promptText="designation">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </TextArea>
      <Label fx:id="error" layoutX="227.0" layoutY="73.0" prefHeight="30.0" prefWidth="200.0" />
      <Button fx:id="buttonAjoute" layoutX="267.0" layoutY="347.0" mnemonicParsing="false" onAction="#ajouterProduit" text="Ajouter Produit">
